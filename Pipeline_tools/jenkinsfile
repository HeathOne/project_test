pipeline {
    agent any

    stages {
        stage('Docker version') {
            steps {
                sh "echo $USER"
                sh 'docker version'
            }
        }
    stages {
        stage('create') {
            steps{
                echo '=====start build======'
                dir ('/Docker_tools'){
                    sh 'docker build -t pira7 '
                }
            } 
        }
    }
}
}
